    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\t\n",
    "def getdata(url):\n",
    "\tr = requests.get(url)\n",
    "\treturn r.text\n",
    "\t\n",
    "htmldata = getdata(\"https://picsart.com/\")\n",
    "soup = BeautifulSoup(htmldata, 'html.parser')\n",
    "for item in soup.find_all('img'):\n",
    "\tprint(item['src'])\n"
   ]
